{
	"name": "@volenday/lumora-icons",
	"version": "1.0.0",
	"private": false,
	"description": "Lumora Icons Component Library",
	"type": "module",
	"main": "dist/index.cjs.js",
	"module": "dist/index.es.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.es.js",
			"require": "./dist/index.cjs.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"prepublishOnly": "npm run build",
		"icons:generate": "svgr --ext tsx --typescript --icon --no-index --template ./scripts/svgr-template.cjs --out-dir src/icons src/assets/icons && node scripts/generate-index.cjs",
		"icons:lint": "node scripts/validate-icons.cjs",
		"preview": "vite preview"
	},
	"peerDependencies": {
		"react": "^19.2.0",
		"react-dom": "^19.2.0"
	},
	"devDependencies": {
		"@svgr/cli": "^8.1.0",
		"@types/react": "^19.2.0",
		"@types/react-dom": "^19.2.0",
		"@vitejs/plugin-react": "^4.3.0",
		"typescript": "^5.6.0",
		"vite": "^7.1.9",
		"vite-plugin-dts": "^4.5.4",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Volenday/lumora-icons.git"
	},
	"author": "Volenday",
	"license": "MIT",
	"keywords": [
		"react",
		"icons",
		"component",
		"library"
	],
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	}
}
